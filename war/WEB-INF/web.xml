<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	
	<servlet>
		<servlet-name>TagTester</servlet-name>
		<servlet-class>calculus.TagTesterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TagTester</servlet-name>
		<url-pattern>/tag-tester</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>About</servlet-name>
		<servlet-class>calculus.general.AboutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>About</servlet-name>
		<url-pattern>/about</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>calculus.profile.ProfileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/user/*</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>ChangeUsername</servlet-name>
		<servlet-class>calculus.profile.ChangeUsernameServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangeUsername</servlet-name>
		<url-pattern>/change-user-name</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>ChangeEmailPreferences</servlet-name>
		<servlet-class>calculus.profile.ChangeEmailPreferencesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangeEmailPreferences</servlet-name>
		<url-pattern>/change-email-preferences</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>PageNotFound</servlet-name>
		<servlet-class>calculus.PageNotFoundServlet</servlet-class>
	</servlet>	
	<servlet-mapping>
		<servlet-name>PageNotFound</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

</web-app>
